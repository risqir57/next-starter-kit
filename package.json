{
  "name": "next-starter-kit",
  "version": "0.1.0",
  "private": false,
  "author": {
    "name": "Risqi Romadhoni",
    "email": "risqir57@gmail.com",
    "url": "https://risqiromadhoni.vercel.app"
  },
  "engines": {
    "node": ">=16.x <=18.x",
    "yarn": ">=1.22.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/risqir57/next-starter-kit.git"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "list:ts": "tsc",
    "list:prettier": "prettier --config .prettierrc ./src/**/* --check",
    "list:style": "stylelint --config .stylelintrc.json --ignore-path .gitignore \"./src/**/*.css\"",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "test:cov": "jest --coverage",
    "checker": "yarn list:ts && yarn lint && yarn list:prettier && yarn list:style",
    "format:lint": "eslint --fix ./src/**/* --ext .js,.ts,.tsx --quiet",
    "format:prettier": "prettier --config .prettierrc ./src/**/* --write",
    "format:style": "stylelint --config .stylelintrc.json --ignore-path .gitignore \"./src/**/*.css\" --fix",
    "formatter": "yarn format:lint && yarn format:prettier && yarn format:style",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "12.1.4",
    "react": "18.0.0",
    "react-dom": "18.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.0.0",
    "@types/node": "17.0.23",
    "@types/react": "18.0.1",
    "@types/react-dom": "18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^8.13.0",
    "eslint-config-next": "^12.1.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "stylelint": "^14.6.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-prettier": "^2.0.0",
    "typescript": "4.6.3"
  }
}
